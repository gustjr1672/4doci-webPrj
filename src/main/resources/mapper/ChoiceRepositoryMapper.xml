<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.doci.webPrj.user.repository.ChoiceRepository">
    <!-- save --> 

    <insert id="save">
        INSERT INTO choice
            (member_id,
            random_challenge_id,
            start_date,
            end_date)
        VALUES
            (#{c.memberId},
            #{c.randomChallengeId},
            #{c.startDate},
            #{c.endDate})


    </insert>



</mapper>